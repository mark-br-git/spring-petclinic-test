pipeline {
    agent any

      stages {
        stage('Creating tag') {
            steps {
                script {
                withCredentials([usernamePassword(credentialsId: 'git', passwordVariable: 'GIT_PASSWORD', usernameVariable: 'GIT_USERNAME')]) {
                sh("git tag -a some_tag11 -m 'Jenkins'")
                sh('git push https://${GIT_USERNAME}:${GIT_PASSWORD}@github.com/mark-br-git/spring-petclinic-test.git --tags')
                }
                }
             }
           }
         }
}
